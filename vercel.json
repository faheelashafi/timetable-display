{ "version": 2,
   "builds": [
     { "src": "src/**/*", "use": "@vercel/static" },
     { "src": "styles/**/*", "use": "@vercel/static" },
     { "src": "scripts/**/*", "use": "@vercel/static" },
     { "src": "logo.png", "use": "@vercel/static" },
     { "src": "index.html", "use": "@vercel/static" }
   ],
   "routes": [
     { "src": "/", "dest": "/index.html" },
     { "src": "/admin", "dest": "/src/AdminLogin.html" },
     { "src": "/admin-panel", "dest": "/src/AdminPanel.html" },
     { "src": "/(.*)", "dest": "/$1" }
   ]
 }