{
  "version": 2,
  "builds": [
    { "src": "**/*", "use": "@vercel/static" }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/", "dest": "/src/DisplayTimetable.html" },
    { "src": "/admin", "dest": "/src/AdminLogin.html" },
    { "src": "/panel", "dest": "/src/AdminPanel.html" }
  ]
}